{
  "name": "next-discord",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname localhost --port 8080",
    "build": "next build && next start --hostname 127.0.0.1 --port 8080",
    "start": "next start --hostname 127.0.0.1 --port 8080",
    "setup": "prisma generate && cd discord && tsc --build",
    "dbuild": "cd discord && tsc --build",
    "dstart": "node discord/build/index.js",
    "drestart": "cd discord && tsc --build && node ./build/index.js",
    "pull": "prisma db pull --force",
    "studio": "prisma studio --hostname 127.0.0.1 --port 8000"
  },
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "@prisma/client": "^4.5.0",
    "@types/node": "^18.8.5",
    "@vercel/og": "^0.0.19",
    "bcrypt": "^5.1.0",
    "chalk": "^4.1.2",
    "daisyui": "^2.31.0",
    "discord.js": "14.3.0",
    "dotenv": "^16.0.2",
    "next": "12.3.1",
    "next-auth": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "prisma": "^4.5.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  }
}
